{
	"name": "RECS-ReplicationPlugin",
	"tree": {
		"$className": "DataModel",

		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",

			"Packages": {
				"$className": "Folder",

				"ReplicationPlugin": {
					"$path": "src"
				},

				"Dev": {
					"$className": "Folder",

					"TestEZ": {
						"$path": "modules/TestEZ/src"
					},
					"lemur": {
						"$path": "modules/lemur/lib"
					},
					"t": {
						"$path": "modules/t/lib"
					}
				}
			}
		}
	}
}
